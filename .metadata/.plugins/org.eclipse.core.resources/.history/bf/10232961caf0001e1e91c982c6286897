package bo;

public class PC {
	private String marque;
	private String type;
	private String disqueDur;
	private String gpu;
	private String cpu;
	
	
	
	public PC(String marque, String type, String disqueDur, String gpu, String cpu) {
		super();
		this.marque = marque;
		this.type = type;
		this.disqueDur = disqueDur;
		this.gpu = gpu;
		this.cpu = cpu;
	}
	
	
}
